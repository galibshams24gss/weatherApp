ame\"\r\n      :counter=\"20\"\r\n      :rules=\"nameRules\"\r\n      class=\"textholder\"\r\n      label=\"Full Name\"\r\n      outlined\r\n      required\r\n    ></v-text-field>\r\n\r\n    <v-text-field\r\n      v-model=\"email\"\r\n      :rules=\"emailRules\"\r\n      label=\"Email Address\"\r\n      class=\"textholder\"\r\n      outlined\r\n      required\r\n    ></v-text-field>\r\n\r\n    <v-textarea\r\n      v-model=\"message\"\r\n      clearable\r\n      clear-icon=\"cancel\"\r\n      :rules=\"messageRules\"\r\n      label=\"Your Message\"\r\n      value=\"\"\r\n      class=\"textholder\"\r\n      outlined\r\n      required\r\n    ></v-textarea>\r\n\r\n    <v-btn\r\n      :disabled=\"!valid\"\r\n      color=\"primary\"\r\n      class=\"mr-4\"\r\n      @click=\"validate\"\r\n      dark\r\n    >\r\n      Submit\r\n    </v-btn>\r\n\r\n    <v-btn\r\n      color=\"indigo\"\r\n      class=\"mr-4\"\r\n      @click=\"reset\"\r\n      dark\r\n    >\r\n      Reset\r\n    </v-btn>\r\n  </v-form>\r\n        </v-row>\r\n    </v-card-actions>\r\n  </v-card>\r\n      </v-col>\r\n  </v-row>\r\n  </v-container>\r\n\r\n  <v-container fluid v-else>\r\n  <v-row\r\n      align=\"center\"\r\n      justify=\"center\"\r\n    >\r\n      <v-col class=\"text-center\" cols=\"12\">\r\n  <v-card\r\n    class=\"mx-auto\"\r\n    dark\r\n    id=\"submitholder\"\r\n    max-width=\"800\"\r\n  >\r\n    <v-card-title>\r\n      <v-icon\r\n        large\r\n        left\r\n        color=\"#AEEA00\"\r\n      >\r\n        mdi-email\r\n      </v-icon>\r\n      <span class=\"title font-weight-light\">Contact Me</span>\r\n    </v-card-title>\r\n\r\n    <v-card-actions>\r\n        <v-row\r\n          align=\"center\"\r\n          justify=\"center\"\r\n        >\r\n        <v-progress-circular\r\n      :rotate=\"180\"\r\n      :size=\"150\"\r\n      :width=\"25\"\r\n      :value=\"value\"\r\n      color=\"lime accent-4\"\r\n      v-if=\"showThanksPic\"\r\n    >\r\n      {{ value }}\r\n    </v-progress-circular>\r\n\r\n    <v-card-text v-if=\"showThanksMsg\" class=\"headline font-weight-bold\">\r\n      Thank you for Contacting\r\n    </v-card-text>\r\n        </v-row>\r\n    </v-card-actions>\r\n  </v-card>\r\n      </v-col>\r\n  </v-row>\r\n  </v-container>\r\n\r\n</template>\r\n\r\n<script>\r\nimport firebase from 'firebase';\r\n\r\n  export default {\r\n    data: () => ({\r\n      valid: true,\r\n      name: '',\r\n      nameRules: [\r\n        v => !!v || 'Name is required',\r\n        v => (v && v.length <= 20) || 'Name must be less than 20 characters',\r\n      ],\r\n      email: '',\r\n      emailRules: [\r\n        v => !!v || 'E-mail is required',\r\n        v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\r\n      ],\r\n      message: '',\r\n      messageRules: [\r\n        v => !!v || 'Message is required',\r\n        v => (v && v.length <= 100) || 'Message must not be empty'\r\n      ],\r\n      showThanks: true,\r\n      showThanksPic: true,\r\n      showThanksMsg: true,\r\n      interval: {},\r\n      value: 0\r\n    }),\r\n\r\n    methods: {\r\n      validate (e) {\r\n        this.$refs.form.validate()\r\n        e.preventDefault()\r\n        const name = this.name;\r\n        const email = this.email;\r\n        const message = this.message;\r\n        this.saveMessage(name, email, message);\r\n        this.showThanks = false;\r\n        this.showThanksPic = true;\r\n        this.showThanksMsg = false;\r\n        this.interval = setInterval(() => {\r\n        if (this.value === 100) {\r\n          this.showThanksPic = false;\r\n          this.showThanksMsg = true\r\n        }\r\n        this.value += 50\r\n      }, 700)\r\n      },\r\n\r\n      saveMessage(name, email, message) {\r\n        const messageRef = firebase.database().ref('messages')\r\n        const newMessageRef = messageRef.push();\r\n        newMessageRef.set({\r\n            name: name,\r\n            email: email,\r\n            message: message\r\n        })\r\n      },\r\n\r\n      reset () {\r\n        this.$refs.form.reset()\r\n      }\r\n    },\r\n\r\n    beforeDestroy () {\r\n      clearInterval(this.interval)\r\n    },\r\n    mounted () {}\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  #submitholder {\r\n    border-radius: 20px;\r\n    background: linear-gradient(to bottom, #3F51B5 0%, #4FC3F7 100%);\r\n  }\r\n\r\n  .v-progress-circular {\r\n    margin: 2rem;\r\n  }\r\n\r\n  .textholder {\r\n    font-weight: bold;\r\n  }\r\n\r\n  #contactholder {\r\n    border-radius: 20px;\r\n    background: linear-gradient(to bottom, #4FC3F7 0%, #2196F3 100%);\r\n  }\r\n</style>"],"sourceRoot":"src/views"}]}